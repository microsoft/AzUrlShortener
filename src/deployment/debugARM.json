{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "frontend": {
           "type": "string",
           "allowedValues": [
               "none",
               "adminBlazorWebsite"
           ],
           "defaultValue": "adminBlazorWebsite"
        }
    },
    "functions": [],
    "variables": {
        "frontendInfo": {
            "adminBlazorWebsite": { 
                "armTemplateUrl": "https://raw.githubusercontent.com/FBoucher/AzUrlShortener/main/src/adminTools/adminBlazorWebsite/deployment/adminBlazorWebsite-deployAzure.json"
            }
        }
    },
    "resources": [],
    "outputs": {
        "frontendTemplateUrl": {
