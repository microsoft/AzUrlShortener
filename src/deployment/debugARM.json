        },
        "azFuncUrl": {
           "type": "string",
           "value": "[resourceId('Microsoft.Web/sites/', 'shortenertools7cw4l')]"
        },
        "functionAppDetail": {
           "type": "object",
           "value": "[reference(resourceId('UrlShortener', 'Microsoft.Web/sites/', 'shortenertools7cw4l'), '2018-02-01')]"
